# Production Environment Variables Template (SQLite3 버전)
# 복사해서 .env 파일로 저장하고 실제 값으로 변경하세요

# Django 기본 설정
DEBUG=False
SECRET_KEY=your-super-secret-key-here-at-least-50-characters-long-and-random

# AWS S3 설정
AWS_ACCESS_KEY_ID=AKIA...
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_STORAGE_BUCKET_NAME=estedu-img
AWS_S3_REGION_NAME=ap-northeast-2
AWS_S3_CUSTOM_DOMAIN=estedu-img.s3.ap-northeast-2.amazonaws.com

# 허용 호스트 (콤마로 구분)
ALLOWED_HOSTS=your-domain.com,your-ec2-public-ip,localhost,127.0.0.1
